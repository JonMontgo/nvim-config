{
  "eslint.autoFixOnSave": true,
  "eslint.filetypes": [
    "javascript",
    "typescript",
    "typescriptreact",
    "javascriptreact"
  ],
  "python.linting.flake8Enabled": true,
  "python.linting.flake8Path": "flake8",
  "python.pythonPath": "/usr/bin/python3.9",
  "coc.preferences.formatOnSaveFiletypes": [
    "go",
    "typescript",
    "javascript",
    "json"
  ],
  "tailwindCSS.headwind.runOnSave": false,
  "yaml.schemas": {
    "http://json.schemastore.org/composer": "/*",
    "kubernetes": [
      "/*kube*.yaml",
      "/*deployment.yaml",
      "/*service.yaml",
      "/*configMap.yaml",
      "/*configmap.yaml",
      "/*secret.yaml",
      "/*secrets.yaml",
      "/*managedcert.yaml",
      "/*redis.yaml",
      "/*app.yaml",
      "/*nginx.yaml",
      "/*ingress.yaml"
    ]
  },
  "languageserver": {
    "solidity": {
      "command": "/home/jonathan/Downloads/solc-linux64",
      "args": ["--lsp"],
      "trace.server": "verbose",
      "rootPatterns": [".git/"],
      "filetypes": ["solidity", "sol"]
    }
  }
}
